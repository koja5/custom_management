{
  "pageSize": 10,
  "request": {
    "type": "GET",
    "api": "/api/getDocuments",
    "parameters": ["getMe"],
    "fields": "",
    "root": ""
  },
  "paging": {
    "enable": true,
    "settings": {
      "pageSize": 10
    }
  },
  "sorting": {
    "enable": true,
    "initalSorting": {
      "columns": [
        {
          "field": "date",
          "direction": "Descending"
        }
      ]
    }
  },
  "grouping": {
    "enable": true,
    "group": [{ "field": "processConfigurationName" }]
  },
  "filtering": {
    "enable": false
  },
  "customGrouping": false,
  "allowResizing": true,
  "reorderable": true,
  "selectable": true,
  "scrollable": false,
  "hideable": true,
  "refresh": false,
  "reset": true,
  "toolbar": ["Print", "Search"],
  "editSettings": {
    "allowEditing": false,
    "allowAdding": false,
    "allowDeleting": false,
    "showDeleteConfirmDialog": false,
    "mode": "Dialog"
  },
  "columns": [
    {
      "title": "date",
      "width": 120,
      "filterable": true,
      "hideable": true,
      "type": "date",
      "class": "text-align-right",
      "defaultValue": "no date",
      "field": "date",
      "format": { "type": "date", "format": "dd/MM/yyyy" }
    },
    {
      "title": "documentName",
      "width": 250,
      "filterable": true,
      "hideable": true,
      "type": "string",
      "class": "text-align-right",
      "field": "name"
    },
    {
      "title": "documentDescription",
      "width": 250,
      "filterable": true,
      "hideable": true,
      "type": "string",
      "class": "text-align-right",
      "field": "description"
    },
    {
      "title": "previewDocument",
      "width": 250,
      "filterable": true,
      "hideable": true,
      "type": "documentPreview",
      "class": "text-align-right",
      "classIcon": "fa fa-user",
      "field": "name"
    }
  ],
  "configField": []
}
