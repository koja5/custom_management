    "parameters": ["superadmin"],
    "fields": "",
    "root": ""
  },
  "paging": {
    "enable": true,
    "settings": {
      "pageSize": 10
    }
  },
  "sorting": {
    "enable": false
  },
  "grouping": {
    "enable": true,
    "group": [{ "field": "processConfigurationName" }]
